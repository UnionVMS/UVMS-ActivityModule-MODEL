			<xsd:element name="ServiceType" type="udt:CodeType"/>
			<xsd:element name="ObjAcronym" type="udt:IDType"/>
			<xsd:element name="Version" type="udt:TextType" minOccurs="0"/>
			<xsd:element name="Date" type="udt:DateType" minOccurs="0"/>
			<xsd:element name="Language" type="udt:CodeType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
