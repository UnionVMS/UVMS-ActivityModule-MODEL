			<xsd:element name="MDR-Query" type="rsm:MDR-QueryType"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ================================================================== -->
	<!-- ===== Type Definition: MDR-QueryType                         ===== -->
